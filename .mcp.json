{
  "mcpServers": {
    "next-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"]
    },
    "shadcn": {
      "type": "stdio",
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    },
    "prisma": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "prisma", "mcp"]
    }
  }
}
